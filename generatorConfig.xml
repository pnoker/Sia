<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<!-- 用于配置数据库连接，分别为MySql、Oracle和SqlServer，三者选择其一，与下面的数据库对应 -->
	<!-- MySql连接 -->
	<classPathEntry location="src/main/webapp/WEB-INF/lib/mysql-connector-java-5.1.36.jar" />

	<!-- Oracle连接 -->
	<!-- <classPathEntry location="src/main/webapp/WEB-INF/lib/ojdbc14.jar" /> -->

	<!-- SQLServer连接 -->
	<!-- <classPathEntry location="src/main/webapp/WEB-INF/lib/sqljdbc4.jar" /> -->

	<context id="DBTableOne" targetRuntime="MyBatis3">
		<!-- 抑制警告 -->
		<property name="suppressTypeWarnings" value="true" />
		<!-- generate entity时，生成hashcode和equals方法 -->
		<plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin" />
		<!-- generate entity时，生成serialVersionUID -->
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
		<!-- 这个插件只会增加字符串字段映射到一个JDBC字符的方法 -->
		<plugin type="org.mybatis.generator.plugins.CaseInsensitiveLikePlugin" />
		<!-- genenat entity时,生成toString -->
		<plugin type="org.mybatis.generator.plugins.ToStringPlugin" />

		<!-- 去掉注释 -->
		<commentGenerator>
			<property name="suppressDate" value="true" />
			<property name="suppressAllComments" value="true" />
		</commentGenerator>

		<!-- 配置数据库连接方式 ，三者选择其一，与上面的Classpath对应 -->
		<!-- MySQL数据库连接配置 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3306/sia" userId="root" password="123456" />

		<!-- Oraclel数据库连接配置 -->
		<!-- <jdbcConnection driverClass="oracle.jdbc.driver.OracleDriver" connectionURL="jdbc:oracle:thin:@localhost:8010:vis" userId="scott" password="tiger" /> -->

		<!-- SQLServer数据库连接配置 -->
		<!-- <jdbcConnection driverClass="com.microsoft.sqlserver.jdbc.SQLServerDriver" connectionURL="jdbc:sqlserver://localhost:1433;Database=opcVis" userId="sa" password="123456" /> -->

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		<javaModelGenerator targetPackage="com.Sia.model" targetProject="src/main/java">
			<property name="enableSubPackages" value="false" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<sqlMapGenerator targetPackage="com.Sia.mapping" targetProject="src/main/mapping">
			<property name="enableSubPackages" value="false" />
		</sqlMapGenerator>
		<javaClientGenerator targetPackage="com.Sia.dao" targetProject="src/main/java" type="XMLMAPPER">
			<property name="enableSubPackages" value="false" />
		</javaClientGenerator>
		<!-- <table tableName="menus" domainObjectName="Menus" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false" 
			/> -->
		<table tableName="users" domainObjectName="Users" enableCountByExample="true" enableUpdateByExample="true" enableDeleteByExample="true" enableSelectByExample="true" selectByExampleQueryId="true" />
		<!-- <table tableName="users" domainObjectName="Users" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false" 
			/> -->
	</context>
</generatorConfiguration>